<div class="row text-center">
	<ul class="pagination mx-auto">
		<li class="page-item <%if(1==pagination.curPage){%>disabled<%}%>" title="Trang Ä‘áº§u">
			<a class="page-link" href="<%=pagination.url+'1'%>" data-url="<%=pagination.url+'1'%>">
				<i class="fas fa-angle-double-left" aria-hidden="true"></i>
			</a>
		</li>
		<li class="page-item <%if(1==pagination.curPage){%>disabled<%}%>">
			<a class="page-link" href="<%=pagination.url%><%=(pagination.curPage-1)%>" data-url="<%=pagination.url%><%=(pagination.curPage-1)%>">
				<i class="fas fa-chevron-left" aria-hidden="true"></i>
			</a>
		</li>
		<%for(var i=pagination.curPage-4;i<pagination.curPage;i++){
			if(i>=1){%>
			<li class="page-item">
				<a class="page-link" href="<%=pagination.url%><%=i%>" data-url="<%=pagination.url%><%=i%>"><%=i%></a>
			</li>
			<%}
		}%>
		<li class="page-item active">
			<a class="page-link" href="#" data-url=""><%=pagination.curPage%></a>
		</li>
		<%for(var i=pagination.curPage+1;(i<=pagination.totalPage)&&(i-pagination.curPage)<=4;i++){%>
		<li class="page-item">
			<a class="page-link" href="<%=pagination.url%><%=i%>" data-url="<%=pagination.url%><%=i%>"><%=i%></a>
		</li>
		<%}%>
		<li class="page-item <%if(pagination.totalPage==pagination.curPage){%>disabled<%}%>">
			<a class="page-link" href="<%=pagination.url+(pagination.curPage+1)%>" data-url="<%=pagination.url+(pagination.curPage+1)%>">
				<i class="fa fa-chevron-right" aria-hidden="true"></i>
			</a>
		</li>
		<li class="page-item <%if(pagination.totalPage==pagination.curPage){%>disabled<%}%>" title="Trang cuá»‘i">
			<a class="page-link" href="<%=pagination.url%><%=(pagination.totalPage)%>" data-url="<%=pagination.url%><%=(pagination.totalPage)%>">
				<i class="fa fa-angle-double-right" aria-hidden="true"></i>
			</a>
		</li>
	</ul>
</div>