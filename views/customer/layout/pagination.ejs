<div class="row text-center">
	<ul class="pagination mx-auto">
		<li class="page-item <%if(1==pagination.curPage){%>disabled<%}%>">
			<a class="page-link" href="<%=pagination.url+'1'%>">
				<i class="fas fa-angle-double-left" aria-hidden="true"></i>
			</a>
		</li>
		<li class="page-item <%if(1==pagination.curPage){%>disabled<%}%>">
			<a class="page-link" href="<%=pagination.url%><%=(pagination.curPage-1)%>">
				<i class="fas fa-chevron-left" aria-hidden="true"></i>
			</a>
		</li>
		<%for(var i=pagination.curPage-1;(i>=1)&&(pagination.curPage-i)<=4;i--){%>
		<li class="page-item">
			<a class="page-link" href="<%=pagination.url+i%><%=i%>"><%=i%></a>
		</li>
		<%}%>
		<li class="page-item active">
			<a class="page-link" href="#"><%=pagination.curPage%></a>
		</li>
		<%for(var i=pagination.curPage+1;(i<=pagination.totalPage)&&(i-pagination.curPage)<=4;i++){%>
		<li class="page-item">
			<a class="page-link" href="<%=pagination.url%><%=i%>"><%=i%></a>
		</li>
		<%}%>
		<li class="page-item <%if(pagination.totalPage==pagination.curPage){%>disabled<%}%>">
			<a class="page-link" href="<%=pagination.url%><%=(pagination.curPage+1)%>">
				<i class="fa fa-chevron-right" aria-hidden="true"></i>
			</a>
		</li>
		<li class="page-item <%if(pagination.totalPage==pagination.curPage){%>disabled<%}%>">
			<a class="page-link" href="<%=pagination.url%><%=(pagination.totalPage)%>">
				<i class="fa fa-angle-double-right" aria-hidden="true"></i>
			</a>
		</li>
	</ul>
</div>